{
  "name": "@identixone/ui-kit",
  "version": "0.0.1",
  "description": "UI kit for Identix web apps",
  "author": "Dmitriy Balashov üë∑üèø‚Äç‚ôÇ <mockedcat@gmail.com>",
  "license": "MIT",
  "browser": "./src/index.js",
  "module": "./src/index.js",
  "files": [
    "src/*",
    "!/**/__tests__"
  ],
  "scripts": {
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch",
    "test:coverage": "better-npm-run test:coverage",
    "lint": "eslint src",
    "prettier": "prettier \"**/*.+(js|jsx|json)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run test && npm run prettier -- --list-different",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook -c .storybook -o docs"
  },
  "betterScripts": {
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "jest --watch",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:coverage": {
      "command": "jest --collectCoverage",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "peerDependencies": {
    "formik": ">=2.0",
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-router-dom": ">=5.0",
    "prop-types": ">=15.7",
    "styled-components": ">=5.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "browser-cookie-lite": "^1.0.4",
    "date-fns": "^2.11.1",
    "dayjs": "^1.8.23",
    "downshift": "^5.0.5",
    "i18next": "^19.3.4",
    "jump.js": "^1.0.2",
    "lodash-es": "^4.17.15",
    "match-sorter": "^4.1.0",
    "moment": "^2.24.0",
    "nouislider": "^14.2.0",
    "nouislider-react": "^3.3.7",
    "rc-notification": "^4.0.0",
    "react-datepicker": "^2.14.1",
    "react-dropzone": "^10.2.2",
    "react-i18next": "^11.3.4",
    "react-qr-svg": "^2.2.1",
    "react-transition-group": "^4.3.0",
    "react-use": "^13.27.1",
    "react-useportal": "^1.0.13",
    "styled-icons": "^10.2.1",
    "styled-normalize": "^8.0.7",
    "styled-theming": "^2.2.0",
    "uuid-validate": "0.0.3",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.4",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "better-npm-run": "^0.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "formik": "^2.1.4",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.4",
    "jest-environment-jsdom": "^25.2.4",
    "jest-environment-jsdom-global": "^1.2.1",
    "jest-styled-components": "^7.0.0",
    "lint-staged": "^10.1.1",
