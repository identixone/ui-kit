{
  "name": "@identixone/ui-kit",
  "version": "0.0.1",
  "description": "UI kit for Identix web apps",
  "author": "Dmitriy Balashov <mockedcat@gmail.com>",
  "license": "MIT",
  "main": "",
  "scripts": {
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "lint": "eslint src",
    "prettier": "prettier \"**/*.+(js|jsx|json)\"",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch",
    "test:debug": "better-npm-run test:debug",
    "test:coverage": "better-npm-run test:coverage",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run test && npm run prettier -- --list-different",
    "precommit": "lint-staged && better-npm-run test:precommit",
    "storybook": "start-storybook"
  },
  "betterScripts": {
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": false
      }
    },
    "test:precommit": {
      "command": "jest --bail",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": false
      }
    },
    "test:watch": {
      "command": "jest --watch",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": false
      }
    },
    "test:coverage": {
      "command": "jest --collectCoverage",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": false
      }
    },
    "test:debug": {
      "command": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": false
      }
    },
    "dev": {
      "command": "webpack-dev-server --progress --config ./config/webpack/webpack.config.dev.js",
      "env": {
        "NODE_ENV": "development",
        "ENV": "development",
        "API_URL": "http://api.testing.identix.local/v1/",
        "DEBUG": true,
        "MAIN_DOMAIN": "identix.local"
      }
    },
    "build": {
      "command": "webpack --config ./config/webpack/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@testing-library/jest-dom": "^4.0.1",
    "change-case": "^3.1.0",
    "dayjs": "^1.8.15",
    "downshift": "^3.2.12",
    "formik": "^1.5.8",
    "jump.js": "^1.0.2",
    "lodash-es": "^4.17.15",
    "nouislider": "^14.0.2",
    "nouislider-react": "^3.3.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "rc-notification": "^3.3.1",
    "react": "^16.9.0",
    "react-dropdown": "^1.6.4",
    "react-dropzone": "^10.1.7",
    "react-qr-svg": "^2.2.1",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.2.2",
    "react-use": "^10.8.0",
    "react-webcam": "^1.1.1",
    "styled-components": "^4.3.2",
    "styled-icons": "^8.3.0",
    "styled-theming": "^2.2.0",
    "uuid-validate": "0.0.3",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@sentry/webpack-plugin": "^1.8.0",
    "@storybook/react": "^5.1.11",
    "@testing-library/react": "^9.1.3",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "better-npm-run": "^0.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.2.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^5.0.0",
    "inline-manifest-webpack-plugin": "^4.0.2",
    "jest": "^24.9.0",
    "jest-environment-jsdom": "^24.9.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.2.3",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "style-loader": "^1.0.0",
    "test-data-bot": "^0.8.0",
    "to-boolean": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  }
}
