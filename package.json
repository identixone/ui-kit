{
  "name": "@identixone/ui-kit",
  "version": "0.0.1",
  "description": "UI kit for Identix web apps",
  "author": "Dmitriy Balashov üë∑üèø‚Äç‚ôÇ <mockedcat@gmail.com>",
  "license": "MIT",
  "browser": "./src/index.js",
  "module": "./src/index.js",
  "files": [
    "src/*",
    "!/**/__tests__"
  ],
  "scripts": {
    "build": "better-npm-run build",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch",
    "test:coverage": "better-npm-run test:coverage",
    "lint": "eslint src",
    "prettier": "prettier \"**/*.+(js|jsx|json)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run test && npm run prettier -- --list-different",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook -c .storybook -o docs"
  },
  "betterScripts": {
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "jest --watch",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:coverage": {
      "command": "jest --collectCoverage",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "build": {
      "command": "webpack --config ./webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "peerDependencies": {
    "formik": ">=1.5.8",
    "react": ">=16.9",
    "react-dom": ">=16.9",
    "react-router-dom": ">=5.0",
    "prop-types": ">=15.7"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "browser-cookie-lite": "^1.0.4",
    "dayjs": "^1.8.16",
    "downshift": "^3.3.5",
    "jump.js": "^1.0.2",
    "lodash-es": "^4.17.15",
    "match-sorter": "^4.0.2",
    "moment": "^2.24.0",
    "nouislider": "^14.0.3",
    "nouislider-react": "^3.3.5",
    "rc-notification": "^3.3.1",
    "react-dropzone": "^10.1.10",
    "react-qr-svg": "^2.2.1",
    "react-transition-group": "^4.3.0",
    "react-use": "^12.7.2",
    "react-useportal": "^1.0.4",
    "styled-components": "^4.4.0",
    "styled-icons": "^8.6.0",
    "styled-theming": "^2.2.0",
    "uuid-validate": "0.0.3",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-export-namespace-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "better-npm-run": "^0.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.2.0",
    "husky": "^3.0.9",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "jest-environment-jsdom": "^24.9.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "storybook-react-router": "^1.0.8",
    "style-loader": "^1.0.0",
    "test-data-bot": "^0.8.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9"
  }
}
